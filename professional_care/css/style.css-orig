div#container {
	
    margin:0px auto;
	width:1000px;
	height:570px;
	/*border: solid brown;*/
}

#header{
	margin-top:5px;
	margin-left:10px;
	margin-right:10px;
	width:980px;
	height:125px;
	/*border: solid #0F3;*/
	/*background:url(../img/jpg/testfade-head.gif) no-repeat;*/
}
	
#menu{
	/*margin-top:5px;*/
	margin-left:10px;
	margin-right:50px;
	width:980px;
	height:40px;
	/*border: solid #00F;*/
	/*background-color: #FFF;*/
