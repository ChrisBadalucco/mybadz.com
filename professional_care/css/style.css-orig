@charset "UTF-8";
/* CSS Document */

body {
	background: #1961AD 50% 0;
	text-align:center;	
	margin:0px auto;
	background-image:url('img/jpg/pc_bg1.jpg');
	font-family:"Times New Roman", Times, serif;
	
}

